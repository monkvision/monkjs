{
  "name": "@monkvision/common",
  "version": "5.3.1",
  "license": "BSD-3-Clause-Clear",
  "packageManager": "yarn@3.2.4",
  "description": "MonkJs common logic package",
  "author": "monkvision",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib && mkdirp lib",
    "build": "yarn run build:development",
    "build:development": "yarn run clean && tsc -p tsconfig.build.development.json",
    "build:production": "yarn run clean && tsc -p tsconfig.build.production.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --check ./src ./test",
    "prettier:fix": "prettier --write ./src ./test",
    "eslint": "eslint --format=pretty ./src ./test",
    "eslint:fix": "eslint --fix --format=pretty ./src ./test",
    "lint": "yarn run prettier && yarn run eslint",
    "lint:fix": "yarn run prettier:fix && yarn run eslint:fix"
  },
  "dependencies": {
    "@monkvision/analytics": "5.3.1",
    "@monkvision/monitoring": "5.3.1",
    "@monkvision/sights": "5.3.1",
    "@monkvision/types": "5.3.1",
    "i18next": "^23.4.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "pako": "^2.1.0",
    "react-i18next": "^13.2.0"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@monkvision/eslint-config-base": "5.3.1",
    "@monkvision/eslint-config-typescript": "5.3.1",
    "@monkvision/eslint-config-typescript-react": "5.3.1",
    "@monkvision/jest-config": "5.3.1",
    "@monkvision/prettier-config": "5.3.1",
    "@monkvision/test-utils": "5.3.1",
    "@monkvision/typescript-config": "5.3.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30",
    "@types/node": "^24.5.2",
    "@types/pako": "^2",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-utils": "^3.0.0",
    "jest": "^30.2",
    "jest-environment-jsdom": "^30.2",
    "mkdirp": "^1.0.4",
    "prettier": "^2.7.1",
    "regexpp": "^3.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.2"
  },
  "prettier": "@monkvision/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkvision/monkjs.git"
  },
  "bugs": {
    "url": "https://github.com/monkvision/monkjs/issues"
  },
  "homepage": "https://github.com/monkvision/monkjs"
}
