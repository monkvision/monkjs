    "react-native-skeleton-content": "^1.0.24",
    "react-native-svg": "^12.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "^0.17.7",
    "react-native-webview": "11.15.0",
    "react-redux": "^7.2.8",
    "react-responsive": "^9.0.0-beta.6",
    "react-webcam": "^7.0.0",
