{
  "name": "monk-demo-app",
  "version": "5.3.2",
  "license": "BSD-3-Clause-Clear",
  "packageManager": "yarn@3.2.4",
  "description": "MonkJs demo app for Photo capture with React and TypeScript",
  "author": "monkvision",
  "private": true,
  "scripts": {
    "start": "env-cmd -e local vite",
    "build": "tsc -b && vite build",
    "build:development": "env-cmd -e development vite build",
    "build:staging": "env-cmd -e staging vite build",
    "build:preview": "env-cmd -e preview vite build",
    "build:backend-staging-qa": "env-cmd -e backend-staging-qa vite build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "eslint": "eslint --format=pretty ./src",
    "eslint:fix": "eslint --fix --format=pretty ./src",
    "lint": "yarn run prettier && yarn run eslint",
    "lint:fix": "yarn run prettier:fix && yarn run eslint:fix"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@monkvision/analytics": "5.3.2",
    "@monkvision/common": "5.3.2",
    "@monkvision/common-ui-web": "5.3.2",
    "@monkvision/inspection-capture-web": "5.3.2",
    "@monkvision/monitoring": "5.3.2",
    "@monkvision/network": "5.3.2",
    "@monkvision/posthog": "5.3.2",
    "@monkvision/sentry": "5.3.2",
    "@monkvision/sights": "5.3.2",
    "@monkvision/types": "5.3.2",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^30",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1",
    "@types/react-dom": "^19.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.13.2",
    "i18next": "^23.4.5",
    "i18next-browser-languagedetector": "^7.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^13.2.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@monkvision/eslint-config-base": "5.3.2",
    "@monkvision/eslint-config-typescript": "5.3.2",
    "@monkvision/eslint-config-typescript-react": "5.3.2",
    "@monkvision/jest-config": "5.3.2",
    "@monkvision/prettier-config": "5.3.2",
    "@monkvision/test-utils": "5.3.2",
    "@monkvision/typescript-config": "5.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^12.1.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
